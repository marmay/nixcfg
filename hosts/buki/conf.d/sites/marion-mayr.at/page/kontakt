<script>
function toggle_visibility( parentId, childId ) {
  if( window.matchMedia( 'min-width: 1279px' ) )
  {
    var parentElement = document.getElementById( parentId );
    var childElement = document.getElementById( childId );
    if( childElement.style.display === 'block' )
    {
      childElement.style.display = 'none';
      parentElement.style.height = 30;
    }
    else
    {
      childElement.style.display = 'block';
      parentElement.style.height = childElement.offsetHeight + 30;
    }
  }
}
</script>
<script>
function same_size_list( elements, pd ) {
  // Ensures that all link boxes on the page have the same height.
  var maxHeight = 0;
  for( var i = 0; i < elements.length; ++i )
  {
    if( elements[ i ].clientHeight - pd > maxHeight )
    {
      maxHeight = elements[ i ].clientHeight - pd;
    }
  }
  console.log( "Maximum height is " + maxHeight );
  for( var i = 0; i < elements.length; ++i )
  {
    elements[ i ].style.height = maxHeight + "px";
  }
}

function same_size_class( className, pd ) {
  same_size_list( document.getElementsByClassName( className ), pd );
}
</script>
</head>
